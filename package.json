{
  "name": "cypress-api-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "test:ci": "cypress run --reporter mochawesome",
    "test:test": "NODE_ENV=test cypress run",
    "test:test:open": "NODE_ENV=test cypress open",
    "test:test:ci": "NODE_ENV=test cypress run --reporter mochawesome",
    "test:staging": "NODE_ENV=staging cypress run",
    "test:staging:open": "NODE_ENV=staging cypress open",
    "test:staging:ci": "NODE_ENV=staging cypress run --reporter mochawesome",
    "test:prod": "NODE_ENV=production cypress run",
    "test:prod:open": "NODE_ENV=production cypress open",
    "test:prod:ci": "NODE_ENV=production cypress run --reporter mochawesome",
    "report": "mochawesome-merge cypress/reports/*.json > mochawesome-report.json",
    "clean": "rm -rf cypress/reports",
    "lint": "eslint 'cypress/**/*.ts'",
    "lint:fix": "eslint 'cypress/**/*.ts' --fix",
    "prepare": "husky install",
    "format": "prettier --write 'cypress/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "chai": "^5.1.2",
    "cypress-plugin-api": "^2.11.2",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^21.0.3",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@types/xml2js": "^0.4.14",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "ajv": "^8.17.1",
    "cypress": "^13.16.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-cypress": "^4.2.0",
    "husky": "^8.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "string_decoder": "^1.3.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.6.3",
    "webpack": "^5.76.0",
    "xml2js": "^0.6.2",
    "xpath": "^0.0.34"
  }
}
